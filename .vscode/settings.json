{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.mypy_cache": true,
        "**/__pycache__": true,
        "**/.qt_for_python": true,
    },
    "cSpell.words": [
        "favicon",
        "gethostbyname",
        "isnumeric",
        "isroutine",
        "pyinstaller",
        "pymenu",
        "pyqt",
        "qrcode",
        "qtawesome",
        "sqlalchemy",
        "werkzeug"
    ],
    "python.linting.flake8Enabled": false,
    "python.linting.mypyEnabled": true,
    "python.linting.enabled": true
}